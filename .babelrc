{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "alias": {
              "src": "./"
            }
          }
        ]
      ]
    }
  },
  "plugins": [["import", { "libraryName": "antd-mobile" }]]
}
